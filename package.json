    "chalk": "4.1.1",
    "cors": "2.8.5",
    "formik": "2.2.6",
    "hl7-dictionary": "1.0.1",
    "i18next": "20.2.4",
    "i18next-browser-languagedetector": "6.1.1",
    "i18next-scanner": "3.0.0",
    "lodash": "4.17.21",
    "loglevel": "1.7.1",
    "mui-datatables": "3.7.7",
    "notistack": "1.0.9",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.0.9",
    "react-i18next": "11.8.15",
    "react-json-tree": "0.15.0",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "redux-injectors": "1.3.0",
    "redux-localstorage-simple": "2.4.0",
    "redux-saga": "1.1.3",
    "styled-components": "5.3.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "uuid": "8.3.2",
    "web-vitals": "1.1.2",
    "xml-formatter": "2.4.0",
    "xml2js": "0.4.23",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.169",
    "@types/node": "15.3.0",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/react-test-renderer": "17.0.1",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "@types/styled-components": "5.1.9",
    "@types/testing-library__jest-dom": "5.9.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.0",
    "body-parser": "1.19.0",
    "concurrently": "6.1.0",
    "cookie": "0.4.1",
    "cookie-parser": "1.4.5",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "7.2.0 || don't update this package unless you are ready",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "express": "4.17.1",
    "fontfaceobserver": "2.1.0",
    "http-proxy-middleware": "2.0.0",
    "husky": "6.0.0",
    "inquirer": "8.0.0",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.4",
    "lint-staged": "11.0.0",
    "miragejs": "0.1.41",
    "node-plop": "0.26.2",
    "nodemon": "2.0.7",
    "path": "0.12.7",
    "plop": "2.7.4",
    "prettier": "2.3.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "sanitize.css": "12.0.1",
    "serve": "11.3.2",
    "set-cookie-parser": "2.4.8",
    "shelljs": "0.8.4",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  },
  "scripts": {
    "dev:server": "nodemon proxy-middleware.js",
    "dev:client": "react-scripts start",
    "start": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run dev:client\"",
    "prod": "npm run build && NODE_ENV=production pm2 start proxy-middleware.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "npm run eslint src",
    "lint:fix": "npm run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "update-deps": "ncu -u && npm i",
    "kill": "lsof -ti:5000 | xargs kill"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
